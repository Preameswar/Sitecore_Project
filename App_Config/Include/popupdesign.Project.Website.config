<?xml version="1.0"?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
	<sitecore>
		<sites>
			<site name="popupdesign" patch:after="site[@name='modules_website']"
			  targetHostName="popupdesign.com" enableTracking="true" virtualFolder="/" physicalFolder="/"
			  rootPath="/sitecore/content"
			  startItem="/popupdesign" database="master" domain="extranet"
			  allowDebug="true" cacheHtml="true" htmlCacheSize="50MB" registryCacheSize="0"
